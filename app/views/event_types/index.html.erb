<% @title = $event_types_title + $generic_title %>

<div class="container" id="index">
	
	<div class="row">
	
		<div class="col-md-12">

			<h1><%= $event_types_title %></h1>
		
			<div id="all-event-types">
				<% @event_types.each do |item| %>
			
					<hr>

					<div class="event-types">
						<div class="event-types-title">
							<div class="history-event-date">
								<p><%= item.event_type %></p>
								<p class="object-id">ID <%= item.id %></p>
							</div>
						</div>
					</div>
						
				<% end %>
			</div>
	
			<hr>
			
		</div>
	
	</div>
	
</div>